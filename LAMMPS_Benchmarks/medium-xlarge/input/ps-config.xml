<Flavor> 0 </Flavor>


<BootstrapScript> 
clear
units metal
dimension 3
boundary p p p
atom_style atomic
atom_modify map array sort 0 0.0
read_data ./relaxed.4V_1500K_0He_1.dat
mass 1 183.841
pair_style snap
pair_coeff * * W_milady.snapcoeff W_milady.snapparam W


neighbor 2.0 bin
neigh_modify every 1 delay 0 check yes
run 0
</BootstrapScript>


<InitScript>
clear
units metal
dimension 3
boundary p p p
atom_style atomic
atom_modify map array sort 0 0.0
read_data %Filename%
mass 1 183.841
pair_style snap
pair_coeff * * W_milady.snapcoeff W_milady.snapparam W



neighbor 2.0 bin
neigh_modify every 1 delay 0 check yes
run 0



</InitScript>


<PostInitScript>

</PostInitScript>

<MDScript>
timestep %Timestep%
fix T all langevin %Temperature% %Temperature% %Friction% %RANDU% gjf no
fix NVE all nve
run %Nsteps%
unfix NVE
unfix T
</MDScript>


<MinScript>
minimize %Tolerance% %Tolerance% 1000 1000
#minimize %Tolerance% %Tolerance% 1000 1000
</MinScript>

<WriteScript>
write_data %Filename%
</WriteScript>



<VelocityInitScript>
velocity    all create %InitTemperature%  %RANDU% dist gaussian loop local
</VelocityInitScript>


